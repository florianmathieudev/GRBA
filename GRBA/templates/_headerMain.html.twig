{% block body %}

<header id="header">
    
{% if  headers|length != 0 %}
    {% for header in headers %}
        <div class="section">
            <a href="/"> <img id="logo" src="{{ asset('/image/GBRA.svg')}}" alt=""> </a>
            <img class="imgHeader" src="{{ asset('image/header/' ~ header.picturePath ) }}">
            <div class="fixed">
                <div class="bg1">
                    <p><strong>{{ header.text|nl2br }}</strong></p>
                </div>
            </div>
        </div>
    {% endfor %}
{% endif %}

</header>

{% endblock %}
