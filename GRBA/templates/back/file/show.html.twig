{% extends 'base.html.twig' %}

{% block title %}File{% endblock %}

{% block body %}
<div class="container-fluid">
    <div class="row ">
        <div class="col-lg-12">  
            <div class="row ">
                <div class='test col-12'>
                    <h1>Fichier</h1>
                    <table class="table">
                        <tbody>
                            <tr>
                                <th>Path</th>
                                <td>{{ file.path }}</td>
                            </tr>
                            <tr>
                                <th>Name</th>
                                <td>{{ file.name }}</td>
                            </tr>
                        </tbody>
                    </table>
                    {{ include('back/file/_delete_form.html.twig') }}
                    <a href="{{ path('file_index') }}">Revenir a la Liste</a>
                </div>
            </div>
        </div>
    </div>      
</div>   
{% endblock %}
